{
  "name": "@kotonosora/nara-vite-react-boilerplate",
  "version": "4.6.20",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:KotonoSora/nara-vite-react-boilerplate.git"
  },
  "type": "module",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalog": {
      "date-fns": "4.1.0"
    }
  },
  "scripts": {
    "build": "react-router build",
    "coverage": "vitest run --coverage",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "lint": "tsc --noEmit",
    "lint:commitlint": "commitlint --edit",
    "prepare": "husky",
    "start": "wrangler dev",
    "test": "vitest",
    "pretypecheck": "wrangler types && node packages/i18n-locales/scripts/generate-i18n-types.mjs",
    "typecheck": "react-router typegen",
    "posttypecheck": "tsc --noEmit",
    "wrangler:types": "wrangler types"
  },
  "dependencies": {
    "@hono/zod-validator": "0.7.6",
    "@tanstack/react-table": "8.21.3",
    "drizzle-orm": "~0.45.1",
    "hono": "4.11.5",
    "isbot": "5.1.33",
    "qrcode.react": "4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0",
    "react-virtuoso": "4.18.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@cloudflare/vite-plugin": "1.21.2",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@mdx-js/mdx": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@mdx-js/rollup": "3.1.1",
    "@react-router/dev": "7.12.0",
    "@react-router/fs-routes": "7.12.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "24.10.9",
    "@types/qrcode.react": "3.0.0",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/ui": "3.2.4",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "11.0.0",
    "drizzle-kit": "~0.31.8",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "mdx-mermaid": "2.0.3",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "rehype-highlight": "7.0.2",
    "rehype-mathjax": "7.1.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "remark-mdx-frontmatter": "5.2.0",
    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-babel": "1.4.1",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "3.2.4",
    "wrangler": "4.60.0"
  },
  "engines": {
    "node": "24.10.0"
  }
}
