    "@react-router/fs-routes": "7.13.0",
    "@types/node": "catalog:",
    "@types/qrcode.react": "3.0.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-istanbul": "4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "11.0.0",
    "drizzle-kit": "~0.31.9",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
