    "@kotonosora/qr-generator": "workspace:*",
    "@kotonosora/scheduler": "workspace:*",
    "@kotonosora/seo": "workspace:*",
    "@kotonosora/ui": "workspace:*",
    "@kotonosora/utils": "workspace:*",
    "@tailwindcss/vite": "4.2.0",
    "@tanstack/react-table": "8.21.3",
    "clsx": "catalog:",
    "date-fns": "catalog:",
    "drizzle-orm": "~0.45.1",
    "highlight.js": "11.11.1",
    "hono": "4.12.0",
    "isbot": "5.1.35",
    "lucide-react": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
    "react-router": "7.13.0",
    "recharts": "catalog:",
    "remix-themes": "catalog:",
    "sonner": "catalog:",
    "tailwindcss": "catalog:",
    "zod": "catalog:"
  },
