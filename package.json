      "remix-themes": "2.0.4",
      "sonner": "2.0.7",
      "tailwindcss": "4.1.18",
      "zod": "4.3.6"
    }
  },
  "scripts": {
    "build": "react-router build",
    "coverage": "vitest run --coverage",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "lint": "tsc --noEmit",
    "lint:commitlint": "commitlint --edit",
    "prepare": "husky",
    "start": "wrangler dev",
    "test": "vitest",
    "pretypecheck": "wrangler types && node packages/i18n-locales/scripts/generate-i18n-types.mjs",
    "typecheck": "react-router typegen",
    "posttypecheck": "tsc --noEmit",
    "wrangler:types": "wrangler types"
  },
  "dependencies": {
    "@hono/zod-validator": "0.7.6",
    "@hookform/resolvers": "catalog:",
    "@kotonosora/blog": "workspace:*",
