{
  "name": "@kotonosora/nara-vite-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "git@github.com:KotonoSora/nara-vite-react-boilerplate.git"
  },
  "private": true,
  "version": "pre-4.0.0",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "bun run build && wrangler deploy",
    "dev": "react-router dev",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc -b",
    "lint": "tsc --noEmit",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@react-router/node": "7.1.3",
    "@react-router/serve": "7.1.3",
    "drizzle-orm": "~0.36.3",
    "isbot": "5.1.17",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20241112.0",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@react-router/dev": "7.1.3",
    "@tailwindcss/vite": "4.0.0",
    "@types/node": "20",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "dotenv-cli": "7.4.3",
    "drizzle-kit": "~0.28.1",
    "husky": "9.1.7",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.0.0",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "3.87.0"
  },
  "engines": {
    "node": "22.13.1"
  }
}
