{
  "name": "@kotonosora/nara-vite-react-boilerplate",
  "version": "4.6.22",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:KotonoSora/nara-vite-react-boilerplate.git"
  },
  "type": "module",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalog": {
      "@hookform/resolvers": "5.2.2",
      "@types/node": "24.10.9",
      "@types/react": "19.2.10",
      "@types/react-dom": "19.2.3",
      "clsx": "2.1.1",
      "date-fns": "4.1.0",
      "lucide-react": "0.563.0",
      "react": "19.2.4",
      "react-dom": "19.2.4",
      "react-hook-form": "7.71.1",
