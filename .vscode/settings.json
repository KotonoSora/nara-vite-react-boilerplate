{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "prettier.configPath": "prettier.config.mjs",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "cSpell.words": [
    "clsx",
    "cssnano",
    "kotonosora",
    "shadcn",
    "sonarqube",
    "tailwindcss",
    "testid"
  ]
}
